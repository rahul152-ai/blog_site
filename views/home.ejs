<%- include('partials/header'); -%>
    <h1>HOME PAGE</h1>
    <p>
        <%=Home%>
    </p>
    <% posts.forEach(function(post){ %>
        <h1>
            <%=post.title%>
        </h1>
        <p>
            <%=post.content.substring(0,100)+"..."%>
            <a href="post/<%=post.title%>">Read</a>
        </p>

    <% }) %>


<%- include('partials/footer'); -%>